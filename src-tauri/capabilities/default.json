{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "opener:default",
    "store:default",
    "http:default",
    {
      "identifier": "http:allow-fetch",
      "allow": [
        {
          "url": "https://generativelanguage.googleapis.com/**"
        }
      ]
    },
    {
      "identifier": "http:allow-fetch-send",
      "allow": [
        {
          "url": "https://generativelanguage.googleapis.com/**"
        }
      ]
    }
  ]
}